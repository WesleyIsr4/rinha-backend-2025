# Application
NODE_ENV=development
PORT=9999
LOG_LEVEL=info

# Payment Processors
DEFAULT_PROCESSOR_URL=http://payment-processor-default:8080
FALLBACK_PROCESSOR_URL=http://payment-processor-fallback:8080

# Database (PostgreSQL)
DB_HOST=postgres
DB_PORT=5432
DB_NAME=rinha_backend
DB_USER=rinha_user
DB_PASSWORD=rinha_password

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Health Check
HEALTH_CHECK_INTERVAL=5000
HEALTH_CHECK_TIMEOUT=5000

# Payment Processing
PAYMENT_TIMEOUT=10000
MAX_RETRIES=3 